{
    "name": "justblink/generic-api",
    "description": "Creates an api, just add orm objects",
    "license": "MIT",
    "require": {
        "tina4stack/tina4php": "2.0.82"
    },
    "scripts": {
        "tina4": "tina4",
        "tina4service": "tina4service",
        "test": "@tina4 tests:run",
        "test:verbose": "@tina4 tests:verbose",
        "initialize": "@tina4 initialize:run",
        "start": "@tina4 webservice:run",
        "start-service": "@tina4service"
    },
    "autoload": {
        "classmap": [
            "src/*"
        ],
        "files": [
            "loadModule.php"
        ],
        "psr-4": {
            "\\": [
                "src/",
                "src/app/",
                "src/orm/",
                "src/routes"
            ]
        }
    },
    "config": {
        "process-timeout": 0
    }
}